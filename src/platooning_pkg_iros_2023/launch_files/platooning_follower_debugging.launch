<launch>
	<arg name="car_numb" value="$(env car_number_str)" />

  	<include 
		file="$(find lidar_ros)/launch/X4.launch">
  	</include>

  	<include 
		file="$(find apriltag_ros)/launch/continuous_detection.launch">
  	</include>

	<node
		pkg="cytron_jetracer"
		type="racecar_with_safety_universal.py"
		name="$(eval 'racecar_with_safety' + arg('car_numb'))" >
	</node>

	<node
		pkg="cytron_jetracer"
		type="publish_velocity_universal.py"
		name="$(eval 'velocity_publisher' + arg('car_numb'))" >
	</node>


	<node
		pkg="auto_jetracer"
		type="lidar_process"
		name="$(eval 'lidar_process_node' + arg('car_numb'))" >
	</node>
	



</launch>
